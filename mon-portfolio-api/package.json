{
  "name": "mon-portfolio-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "generate-api-key": "node scripts/generateApiKey.js"
  },
  "jest": {
    "testTimeout": 10000,
    "testMatch": [
      "**/tests/**/*.test.js",
      "**/__tests__/**/*.test.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx",
      "node"
    ],
    "testEnvironment": "node",
    "detectOpenHandles": true,
    "collectCoverage": true,
    "coverageReporters": [
      "text",
      "lcov"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "mongoose": "^8.10.1",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "jest": "^29.0.0",
    "nodemon": "^3.1.9",
    "supertest": "^7.0.0",
    "bcrypt": "^5.1.1"
  },
  "directories": {
    "test": "tests"
  }
}
